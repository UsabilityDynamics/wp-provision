{
  "name": "wp-provision",
  "version": "0.1.1",
  "config": {
    "out": "./components",
    "namespace": "Provision",
    "basename": "app"
  },
  "build": {
    "scripts": {
      "out": "./public/scripts",
      "settings": {
        "minify": true
      }
    },
    "styles": {
      "out": "./public/styles",
      "settings": {
        "minify": true
      }
    },
    "fonts": {
      "out": "./public/fonts",
      "settings": {}
    },
    "views": {
      "out": "./public/views",
      "settings": {}
    },
    "schemas": {
      "out": "./public/schemas",
      "settings": {}
    },
    "libs": {
      "out": "./core/vendor",
      "settings": {
        "autoload": true
      }
    }
  },
  "dependencies": {
    "usabilitydynamics/lib-api": "*",
    "usabilitydynamics/lib-loader": "*",
    "usabilitydynamics/lib-settings": "*",
    "usabilitydynamics/lib-saas": "*",
    "usabilitydynamics/lib-cdn": "*",
    "usabilitydynamics/js-spa": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UsabilityDynamics/wp-provision"
  },
  "license": "MIT"
}
