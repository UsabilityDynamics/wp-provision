{
  "name": "usabilitydynamics/wp-provision",
  "type": "library",
  "description": "WordPress plugin for provisioning SVN and Git repositories.",
  "repository": {
    "type": "git",
    "urL": "https://github.com/UsabilityDynamics/wp-provision"
  },
  "homepage": "https://github.com/UsabilityDynamics/wp-provision",
  "keywords": [
    "wordpress",
    "provision"
  ],
  "minimum-stability": "dev",
  "license": "GPLv2",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "svn",
      "url": "https://github.com/UsabilityDynamics/lib-utility",
      "trunk-path": "trunk",
      "branches-path": "branches",
      "tags-path": "tags"
    },
    {
      "type": "svn",
      "url": "https://github.com/UsabilityDynamics/lib-settings",
      "trunk-path": "trunk",
      "branches-path": "branches",
      "tags-path": "tags"
    },
    {
      "type": "svn",
      "url": "https://github.com/UsabilityDynamics/lib-ui",
      "trunk-path": "trunk",
      "branches-path": "branches",
      "tags-path": "tags"
    }
  ],
  "require": {
    "php": ">=5.3",
    "UsabilityDynamics/lib-utility": "*",
    "UsabilityDynamics/lib-settings": "*",
    "UsabilityDynamics/lib-ui": "*"
  },
  "autoload": {
    "files": [
      "lib/class-bootstrap.php",
      "lib/class-core.php",
      "lib/class-settings.php",
      "lib/class-ui.php",
      "lib/class-utility.php"
    ]
  }
}
